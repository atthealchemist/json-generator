{
  "name": "jsongeneration",
  "displayName": "JsonGeneration",
  "description": "Generate stub json data using templates and so on",
  "version": "0.0.1",
  "publisher": "atthealchemist",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jsongeneration.generate",
    "onLanguage:json"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "title": "JG: Generate JSON",
        "command": "jsongeneration.generate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "gulp",
    "watch": "gulp",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "fixallshit": "npm clean cache --force && rm -rf node_modules/ && npm install",
    "lint": "tslint src/*.ts -t verbose"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "cross-env": "^5.2.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "tslint": "^5.8.0",
    "typescript": "^2.9.2",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "@types/typescript": "^2.0.0",
    "date-time-js": "^1.0.6",
    "moment": "^2.22.2",
    "typescript": "^3.0.1"
  }
}
